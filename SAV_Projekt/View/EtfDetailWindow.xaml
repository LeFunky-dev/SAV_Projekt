<Window x:Class="SAV_Projekt.View.EtfDetailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SAV_Projekt.View"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        DataContext="{Binding Source={StaticResource Locator}, Path=EtfDetail}"
        Title="EtfDetailWindow" Height="450" Width="800">
    <Grid>
        <lvc:CartesianChart Name="ETF" Series="{Binding Series}" LegendLocation="Right">
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="Month" LabelFormatter="{Binding Formatter}">
                    <!--<lvc:Axis.Sections>
                        <lvc:AxisSection Value="41000" StrokeThickness="3" Stroke="Gray"/>
                    </lvc:Axis.Sections>-->
                </lvc:Axis>
            </lvc:CartesianChart.AxisX>
        </lvc:CartesianChart>
    </Grid>
</Window>
